{
  "name": "@codegraph/plugin-python",
  "version": "0.1.0",
  "description": "Python language plugin for CodeGraph",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@codegraph/types": "workspace:*",
    "@codegraph/logger": "workspace:*",
    "tree-sitter": "^0.21.1",
    "tree-sitter-python": "^0.21.0"
  },
  "devDependencies": {
    "typescript": "^5.7.3"
  }
}
