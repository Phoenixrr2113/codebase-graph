{
  "name": "@codegraph/plugin-typescript",
  "version": "0.1.0",
  "description": "TypeScript/JavaScript language plugin for CodeGraph",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@codegraph/types": "workspace:*",
    "@codegraph/logger": "workspace:*",
    "tree-sitter": "^0.21.1",
    "tree-sitter-typescript": "^0.23.2"
  },
  "devDependencies": {
    "typescript": "^5.7.3"
  },
  "peerDependencies": {
    "tree-sitter": "^0.21.0"
  }
}
